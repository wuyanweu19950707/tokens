{
  "toggle": {
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/%F0%9F%A7%A9-Webex-App---Windows?type=design&node-id=46518-104001&mode=design&t=vOXmg0dJGz4PYQdJ-0",
    "toggled": {
      "#normal": {
        "background": "@theme-control-active-normal",
        "thumb-tint": "@theme-text-inverted-normal",
        "border": "@theme-control-active-normal"
      },
      "#hovered": {
        "background": "@theme-control-active-hover",
        "thumb-tint": "@theme-text-inverted-normal",
        "border": "@theme-control-active-hover"
      },
      "#pressed": {
        "background": "@theme-control-active-pressed",
        "thumb-tint": "@theme-text-inverted-normal",
        "border": "@theme-control-active-pressed"
      },
      "#disabled": {
        "background": "@theme-control-active-disabled",
        "thumb-tint": "@theme-text-inverted-disabled",
        "border": "@theme-control-active-disabled"
      }
    },
    "untoggled": {
      "#normal": {
        "background": "@theme-button-secondary-normal",
        "thumb-tint": "@theme-text-primary-normal",
        "border": "@theme-outline-input-normal"
      },
      "#hovered": {
        "background": "@theme-button-secondary-hover",
        "thumb-tint": "@theme-text-primary-normal",
        "border": "@theme-outline-input-normal"
      },
      "#pressed": {
        "background": "@theme-button-secondary-pressed",
        "thumb-tint": "@theme-text-primary-normal",
        "border": "@theme-outline-input-normal"
      },
      "#disabled": {
        "background": "@theme-button-secondary-disabled",
        "thumb-tint": "@theme-text-primary-disabled",
        "border": "@theme-outline-input-normal"
      }
    }
  }
}
